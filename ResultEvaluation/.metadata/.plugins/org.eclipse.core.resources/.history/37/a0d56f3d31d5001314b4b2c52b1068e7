#include "auxilliary.h"
#include "def.h"

using namespace std;

void statistic_known( )
{
	ifstream input;
	input.open("");

	string line;
	int count = 0;
	int count1 = 0;
	int count2 = 0;
	while( getline(input, line))
	{
		vector<string> stritem = split(line, "\t");
		if (stritem[0] == "fly.i2d-yeast.i2d_fly")
		{
			count++;
			double f_score = atof(stritem[7].c_str());
			if (f_score - 0.3 > 0 )
			{
				count1++;
			}
			if (f_score - 0.5 > 0 )
			{
				count2++;
			}
		}

	}
	cout << "Matched solutions:"<< count<<endl;
	cout << "High quality solutions(F-index > 0.3):"<< count1<<endl;
	cout << "High quality solutions(F-index > 0.5):"<< count2<<endl;
}
